{
  "name": "rancher-desktop",
  "version": "0.0.1",
  "main": "background.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "electron": "^10.1.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "follow-redirects": "^1.13.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.0.5",
